{
  "name": "mrk",
  "version": "2.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "mrk": "./dist/bin/mrk.js"
  },
  "license": "MIT",
  "private": false,
  "author": "alekzonder <alekzonder@gmail.com>",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/alekzonder/mrk.git"
  },
  "scripts": {
    "clean": "rm -rf coverage dist reports",
    "_build": "swc src --out-dir dist && npm run updateTscPaths",
    "build": "tsc && npm run updateTscPaths",
    "_watch": "swc src --out-dir dist -w --sync & tsc -w --pretty --skipLibCheck --noEmit",
    "watch": "tsc-watch --onSuccess 'npm run updateTscPaths' --onFailure 'npm run updateTscPaths'",
    "updateTscPaths": "tscpaths -p tsconfig.json -s ./src -o ./dist > /dev/null 2>&1",
    "start": "ts-node src/index.ts",
    "migrate": "typeorm migration:run",
    "jest": "jest --coverage=false --maxWorkers=2",
    "test:jest": "jest --coverage=false --testPathIgnorePatterns integration",
    "test:jest:coverage_report": "JEST_HTML_REPORTERS_PUBLIC_PATH=reports/jest/ jest --maxWorkers=25% --coverage=true  --testPathIgnorePatterns integration --reporters jest-html-reporters",
    "test:jest:coverage": "jest --coverage=true  --testPathIgnorePatterns integration",
    "test:jest:watch": "jest --maxWorkers 2 --watch --coverage=false",
    "test:integration": "jest --coverage=false -w 1 src/spec/integration/",
    "prepublishOnly": "npm run ci:build",
    "ci:build": "npm run clean && npm run lint && npm run build && npm run test:jest:coverage",
    "genPackagelock": "npm i --package-lock-only",
    "lint": "eslint src/",
    "lint:write": "eslint --debug src/ --fix"
  },
  "dependencies": {
    "@0devs/config": "^2.4.0",
    "@0devs/error": "^1.7.0",
    "dayjs": "^1.11.4",
    "express": "^4.18.1",
    "finalhandler": "^1.2.0",
    "fs-extra": "^10.1.0",
    "highlight.js": "^11.6.0",
    "joi": "^17.6.0",
    "klaw": "^4.0.1",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.4",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-ins": "^3.0.1",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-obsidian": "^1.1.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-table-of-contents": "^0.6.0",
    "markdown-it-task-lists": "^2.1.1",
    "nanoid": "^4.0.0",
    "pino": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "serve-index": "^1.9.1",
    "serve-static": "^1.15.0",
    "source-map-support": "^0.5.21",
    "tslog": "^3.3.3",
    "xstate": "4.32.1"
  },
  "devDependencies": {
    "@0devs/package": "^0.18.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.218",
    "@swc/jest": "^0.2.22",
    "@types/express": "^4.17.13",
    "@types/finalhandler": "^1.1.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.6",
    "@types/klaw": "^3.0.3",
    "@types/lodash": "^4.14.182",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.0.6",
    "@types/serve-index": "^1.9.1",
    "@types/serve-static": "^1.13.10",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "babel-plugin-module-resolver": "^4.1.0",
    "chokidar": "^3.5.3",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-sonarjs": "^0.14.0",
    "fast-check": "^3.0.1",
    "jest": "^28.1.3",
    "jest-html-reporters": "^3.0.10",
    "mockdate": "^3.0.5",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^28.0.7",
    "ts-mockito": "^2.6.1",
    "ts-node": "10.9.1",
    "tsc-watch": "^5.0.3",
    "tscpaths": "^0.0.9",
    "typescript": "^4.7.4",
    "utility-types": "^3.10.0"
  }
}
