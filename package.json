{
  "name": "mrk",
  "version": "0.8.0-0",
  "description": "mrk",
  "main": "index.js",
  "scripts": {
    "config": "node ./config.default.js",
    "clean": "rm -rf ./esdoc ./coverage",
    "docs": "rm -rf ./esdoc && ./node_modules/.bin/esdoc ./ -c ./.esdoc.json",
    "docs:open": "npm run docs && open ./esdoc/index.html",
    "tests": "./node_modules/.bin/mocha ./test/",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha ./test/",
    "lint": "./node_modules/.bin/eslint -c .eslintrc.js api init models rest",
    "lint:fix": "./node_modules/.bin/eslint -c .eslintrc.js --fix  api init models rest",
    "postversion": "git push origin && git push --tags"
  },
  "engines": {
    "node": ">=4"
  },
  "bin": {
    "mrk": "./bin/mrk.js"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alekzonder/mrk.git"
  },
  "keywords": [],
  "author": "alekzonder <alekzonder@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": null
  },
  "homepage": null,
  "dependencies": {
    "chokidar": "~1.6.1",
    "commander": "~2.9.0",
    "finalhandler": "~0.5.1",
    "fs-extra": "~2.0.0",
    "highlight.js": "~9.9.0",
    "joi": "~10.2.1",
    "klaw": "~1.3.1",
    "lodash": "4.17.4",
    "maf": "1.0.0-9",
    "markdown-it": "8.3.x",
    "markdown-it-abbr": "~1.0.4",
    "markdown-it-anchor": "4.0.x",
    "markdown-it-container": "~2.0.0",
    "markdown-it-emoji": "~1.3.0",
    "markdown-it-footnote": "~3.0.1",
    "markdown-it-ins": "^2.0.0",
    "markdown-it-mark": "^2.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-task-lists": "~1.4.1",
    "markdown-toc": "^1.1.0",
    "md5": "~2.2.1",
    "moment": "~2.17.1",
    "serve-index": "~1.8.0",
    "superagent": "~3.4.1",
    "uuid": "~3.0.1"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-json-schema": "^1.4.0",
    "codeclimate-test-reporter": "^0.4.0",
    "esdoc": "^0.4.8",
    "esdoc-node": "^1.0.0",
    "eslint": "^3.11.1",
    "istanbul": "^0.4.4",
    "mocha": "^3.2.0"
  }
}
